form>\n    <br>\n    <table class=\"table table-striped\">\n      <tr>\n        <th>Id</th>\n        <th>Assunto</th>\n        <th>Texto</th>\n        <th>Data/hora</th>\n        <th>Usuário</th>\n      </tr>\n      <tr v-for=\"ant in anotacoes\" :key=\"ant.id\">\n        <td>{{ ant.id }}</td>\n        <td>{{ ant.assunto }}</td>\n        <td>{{ ant.texto }}</td>\n        <td>{{ ant.dataHora }}</td>\n        <td>{{ ant.usuario.nome }}</td>\n      </tr>\n    </table>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport { mapState } from 'vuex'\n\nexport default {\n  name: 'anotacoes',\n  data() {\n    return {\n      assunto: '',\n      texto: '',\n      anotacoes: []\n    }\n  },\n  computed: {\n    ...mapState([\n      'usuario'\n    ])\n  },\n  methods: {\n    cadastrar() {\n      axios.post('anotacao/save',\n          {\n            assunto: this.assunto,\n            texto: this.texto,\n            usuario: this.usuario\n          })\n        .then(res => {\n          this.assunto = ''\n          this.texto = ''\n          this.atualizar()\n        })\n        .catch(error => console.log(error))\n    },\n    atualizar () {\n      axios.get('/anotacao/getAll', \n          { headers: { Accept: 'application/json' } })\n        .then(res => {\n          console.log(res)\n          this.anotacoes = res.data\n        })\n        .catch(error => console.log(error))\n    }\n  },\n  created () {\n    this.atualizar()\n  }\n}\n</script>\n"],"sourceRoot":"src/views"}]}